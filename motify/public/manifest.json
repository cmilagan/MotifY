{
  "short_name": "MotifY",
  "name": "Motify",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Chrome extension to remind users via notifications",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["./main.css"]
    }
  ],
  "action": {
    "default_popup": "./popup.html"
  }
}